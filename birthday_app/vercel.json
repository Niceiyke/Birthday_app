{
    "version": 2,
    "builds": [
      {
        "src": "birthday_app/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
          {
            "src": "/(.*)",
            "dest": "birthday_app/wsgi.py"
          }
    ],
    "hooks": {
      "postdeploy": "bash create_superuser.sh"
    }
  
  }
  